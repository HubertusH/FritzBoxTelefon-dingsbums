<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
  <ribbon>
    <tabs>
      <tab idMso="TabContact">
        <group id="TabContact" label="Fritz!Box Telefon-Dingsbums" getVisible="GroupVisible">
          <button id="ButtonC1" imageMso="AutoDial" onAction="OnActionWählen" size="large" label="Wählen" screentip="Fritz!Box Wählclient für Outlook"/>
          <menu id="mnuC01" size="large" imageMso="CheckNames" label="Rückwärtssuche" screentip="Suchen Sie zusätzliche Informationen zu diesem Kontakt mit der Rückwärtssuche.">
            <button id="btnC01" tag="RWS11880" label="11880" onAction="OnActionRWS" screentip="Rückwärtssuche mit &#34;www.11880.com&#34;"/>
            <button id="btnC02" tag="RWSDasOertliche" label="dasÖrtliche" onAction="OnActionRWS" screentip="Rückwärtssuche mit &#34;www.dasoertliche.de&#34;"/>
            <button id="btnC03" tag="RWSDasTelefonbuch" label="dasTelefonbuch" onAction="OnActionRWS" screentip="Rückwärtssuche mit &#34;www.dastelefonbuch.de/&#34;"/>
            <button id="btnC04" tag="RWSTelSearch" label="tel.search.ch" onAction="OnActionRWS" screentip="Rückwärtssuche mit &#34;tel.search.ch&#34;"/>
            <button id="btnC05" tag="RWSAlle" label="Alle" onAction="OnActionRWS" screentip="Rückwärtssuche mit allen Anbietern."/>
          </menu>
          <toggleButton id="InspVIP" label="VIP" onAction="OnActionInspVIP" getPressed="GetPressedVIP" imageMso="Pushpin" size="large" getScreentip="GetScreenTipVIP"/>
          <button id="ButtonC2" imageMso="ShowNotesPage" onAction="OnActionNote" size="large" label="Notiz" screentip="Einen Notizeintrag hinzufügen"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

