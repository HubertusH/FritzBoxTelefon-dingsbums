            <!--Menü mit Schaltflächen des SplitButton-->
            <menu id="mnu_C2">
              <button id="AnrMonBtn_C2" getLabel="GetLabel" onAction="OnActionAnrMonNeustarten" imageMso="RecurrenceEdit" getScreentip="GetScreenTipp"/>
