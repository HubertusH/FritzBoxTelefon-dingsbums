  <ribbon>
    <tabs>
      <tab idMso="TabContacts">
        <group id="TabContact" label="Fritz!Box Telefon-Dingsbums" getVisible="GroupVisible">
          <splitButton id="spb1_C" size="large">
            <!--oberste Schaltfläche des SplitButton-->
            <button id="Button_C1" imageMso="AutoDial" onAction="OnActionWählenExplorer" label="Wählen" screentip="Fritz!Box Wählclient für Outlook"/>
            <!--Menü mit Schaltflächen des SplitButton-->
            <menu id="mnu_C1">
              <button id="Button_C2" label="Direktwahl" onAction="OnActionDirektwahl"/>
            </menu>
          </splitButton>
          <separator id="Seperator_C1"/>
          <dynamicMenu id="dynMwwdh_C" label=" Wahlwiederholung" getContent="DynMenüfüllen" invalidateContentOnDrop="true"/>
          <dynamicMenu id="dynMAnrListe_C" label="Rückruf" getContent="DynMenüfüllen" invalidateContentOnDrop="true"/>
          <dynamicMenu id="dynMVIPListe_C" label="VIP-Liste" getContent="DynMenüfüllenVIP" invalidateContentOnDrop="true"/>
          <separator id="Seperator_C2" getVisible="GetVisibleAnrMonFKT"/>
          <splitButton id="spb2_C" size="normal" getVisible="GetVisibleAnrMonFKT">
            <!--oberste Schaltfläche des SplitButton-->
            <toggleButton id="btnSplit_C" label="Anrufmonitor" onAction="OnActionAnrMonAnAus" getPressed="GetPressed" getImage="getImage"/>
            <!--Menü mit Schaltflächen des SplitButton-->
            <menu id="mnu_C2">
              <button id="AnrMonBtn_C2" label="Neustarten" onAction="OnActionAnrMonNeustarten" imageMso="RecurrenceEdit"/>
            </menu>
          </splitButton>
          <button id="AnrMonBtn_C1" label="Anzeigen" onAction="OnActionAnrMonAnzeigen" imageMso="ClipArtInsert" getVisible="GetVisibleAnrMonFKT"/>
          <button id="AnrMonBtn_C4" label="Journalimport" onAction="OnActionJournalImport" imageMso="NewJournalEntry" getVisible="GetVisibleAnrMonFKT"/>
          <dialogBoxLauncher>
            <button id="Einstellungen_C" screentip="Fritz!Box Telefon-Dingsbums Einstellungen" supertip="Öffnet den Fritz!Box Telefon-Dingsbums Einstellungsdialog" onAction="OnActionEinstellungen" />
          </dialogBoxLauncher>
        </group>
      </tab>
