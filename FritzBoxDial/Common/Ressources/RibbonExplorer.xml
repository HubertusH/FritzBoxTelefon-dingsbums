            <button id="Button_C1" imageMso="AutoDial" onAction="OnActionWählenExplorer" getLabel="GetLabel" getScreentip="GetScreenTipp"/>
            <!--Menü mit Schaltflächen des SplitButton-->
            <menu id="mnu_C1">
