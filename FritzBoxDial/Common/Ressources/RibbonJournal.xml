             <button id="btnJ04" tag="RWSTelSearch" label="tel.search.ch" onAction="OnActionRWS" screentip="Rückwärtssuche mit &#34;tel.search.ch&#34;"/>
             <button id="btnJ05" tag="RWSAlle" label="Alle" onAction="OnActionRWS" screentip="Rückwärtssuche mit allen Anbietern."/>
