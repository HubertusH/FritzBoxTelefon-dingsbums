          <button id="ButtonJ2" imageMso="RecordsSaveAsOutlookContact" onAction="OnActionKontakterstellen" size="large" getEnabled="ButtonEnable" getScreentip="SetScreenTipJournal" getLabel="SetLabelJournal"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
