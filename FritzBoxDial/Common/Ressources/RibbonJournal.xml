          <button id="ButtonJ1" imageMso="AutoDial" onAction="OnActionWählen" size="large" label="Wählen" screentip="Fritz!Box Wählclient für Outlook" getEnabled="ButtonEnableW"/>
           <menu id="mnu01" size="large" imageMso="CheckNames" label="Rückwärtssuche" screentip="Suchen Sie zusätzliche Informationen zu diesem Anrufer mit der Rückwärtssuche." getEnabled="ButtonEnableW">
